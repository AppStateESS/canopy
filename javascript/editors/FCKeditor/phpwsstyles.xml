<Styles>
	<Style name="Image on Left" element="img">
		<Attribute name="class" value="float-left" />
	</Style>
	<Style name="Image on Right" element="img">
		<Attribute name="class" value="float-right" />
	</Style>
	<Style name="Smaller" element="span">
		<Attribute name="class" value="smaller" />
	</Style>
	<Style name="Larger" element="span">
		<Attribute name="class" value="larger" />
	</Style>
</Styles>
