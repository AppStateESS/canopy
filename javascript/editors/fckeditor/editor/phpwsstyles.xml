<Styles>
	<Style name="Remove CSS formatting" element="p">
		<Attribute name="style" value="" />
		<Attribute name="class" value="" />
	</Style>
	<Style name="Clear left" element="p">
		<Attribute name="style" value="clear : left" />
	</Style>
	<Style name="Clear right" element="p">
		<Attribute name="style" value="clear : right" />
	</Style>
	<Style name="Image on Left" element="img">
		<Attribute name="class" value="float-left" />
	</Style>
	<Style name="Image on Right" element="img">
		<Attribute name="class" value="float-right" />
	</Style>
	<Style name="Smaller" element="span">
		<Attribute name="class" value="smaller" />
	</Style>
	<Style name="Larger" element="span">
		<Attribute name="class" value="larger" />
	</Style>
	<Style name="Heading 1" element="h1" />
	<Style name="Heading 2" element="h2" />
	<Style name="Heading 3" element="h3" />
	<Style name="Heading 4" element="h4" />
</Styles>
